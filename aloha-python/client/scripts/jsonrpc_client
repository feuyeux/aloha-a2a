#!/bin/bash
cd "$(dirname "$0")/../.."
if command -v uv &> /dev/null; then
    uv run python -m client --transport jsonrpc --port 13001 --message "Roll a 6-sided dice" 2>/dev/null || python -m client --transport jsonrpc --port 13001 --message "Roll a 6-sided dice"
else
    python -m client --transport jsonrpc --port 13001 --message "Roll a 6-sided dice"
fi
